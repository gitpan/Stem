--- #YAML:1.0
-
 class: Stem::Console
-
 class: Stem::Proc
 name: hello
 args:
    path: bin/hello.sh
    proc_args: 
     - foo
    cell_attr:
     data_addr: console
-
 class: Stem::Proc
 name: hello1
 args:
      path: bin/hello.sh
      proc_args: 
       - bar
      cell_attr:
       data_addr: console
-
 class: Stem::Switch
 name: sw
 args:
  in_map:
     - a
     - [ h, h1 ]
  out_map:
     h: hello
     h1: hello1
